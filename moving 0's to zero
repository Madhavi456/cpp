#include<iostream>
using namespace std;

int main()
{
	int n=5;
	int arr[]={12,0,13,0,15};
	int count =0;
	for(int i=0;i<n;i++)
	cout<<arr[i]<<" ";
	cout<<endl;
	for(int i=0;i<n;i++)
	{
	if(arr[i] != 0)
	{
		swap(arr[count],arr[i]);
		count++;
	}
    }
   	for(int i=0;i<n;i++)
	{
	   cout<<arr[i]<<" ";	
	}	
}
